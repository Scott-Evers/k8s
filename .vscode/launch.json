    "version": "0.2.0",
    "configurations": [


        {
            "type": "node",
            "request": "launch",
            "name": "Data",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/blog_sample_app/data/service.js",
            "cwd": "${workspaceFolder}/blog_sample_app/data"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Biz",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/blog_sample_app/biz_logic/service.js",
            "cwd": "${workspaceFolder}/blog_sample_app/biz_logic"
        }
    ],
    "compounds": [
        {
            "name": "Biz+Data",
            "configurations": ["Data","Biz"]
        }
    ]
}